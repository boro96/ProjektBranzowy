@model IEnumerable<ProjektBranzowy.Models.ScheduleRoom>

    <div class="container p-3">
        <div class="row pt-4">
            <div class="col-6">
                <h2 class="text-primary">Schedule rooms list</h2>
            </div>
            <div class="col-6">
                Create new Item
            </div>
        </div>
        <br />

    </div>
@if(Model.Count()>0)
 {
               
                 <table class="table table-dark table-striped" style="width:100%">
                    <thead>
                        <tr>
                            <th>RoomName</th>
                            <th>WeekDay</th>
                            <th>Entry</th>
                            <th>Exit</th>

                        </tr>
                              </thead>
                    <tbody>
                        @foreach(var scheduleRoom in Model)
                {
                    <tr>
                        <td>@scheduleRoom.Room.Name</td>
                        <td>@scheduleRoom.WeekDayName</td>
                        <td>@scheduleRoom.Start</td>
                        <td>@scheduleRoom.Finish</td>

                    </tr>
                }
                        </tbody>



                    </table>
}
else
            {
                <p>No schedule rooms</p>
            }



      