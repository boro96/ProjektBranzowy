@model IEnumerable<ProjektBranzowy.Models.User>

    <div class="container p-3">
        <div class="row pt-4">
            <div class="col-6">
                <h2 class="text-primary">Users List</h2>
            </div>
            <div class="col-6">
                Create new Item
            </div>
        </div>
        <br />

    </div>
@if(Model.Count()>0)
 {
                <table class="table table-bordered table-striped" style="width:100%">
                    <thead>
                      <tr>
                          <th>
                              Name
                              </th>
                          
                              </tr>
                              </thead>
                    <tbody>
                        @foreach(var user in Model)
                {
                    <tr>
                        <td>@user.Name</td>
                        </tr>
                }
                        </tbody>



                    </table>
}
else
            {
                <p>No Users created yet</p>
            }



      